<!-- 409 = american english -->

<GRAMMAR LANGID="409">
	<DEFINE>
		<ID NAME="RID_Close" VAL="2"></ID>
		<ID NAME="RID_Play" VAL="4"></ID>
		<ID NAME="RID_Stop" VAL="11"></ID>
	</DEFINE>

  <RULE NAME="pausePlay" ID="RID_Play" TOPLEVEL="ACTIVE">
    <RULEREF NAME="ComputerName" URL="XMLMain.xml" />
		<P>play</P>
    <P>pause</P>
	</RULE>
	<RULE NAME="stop" ID="RID_Stop" TOPLEVEL="ACTIVE">
    <RULEREF NAME="ComputerName" URL="XMLMain.xml" />
		<P>stop</P>
	</RULE>
	<RULE NAME="close" ID="RID_Close" TOPLEVEL="ACTIVE">
    <RULEREF NAME="ComputerName" URL="XMLMain.xml" />
		<P>close</P>
	</RULE>
</GRAMMAR>